@model IEnumerable<Manifiesto.Web.Areas.Aereo.Models.guiasListViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Guias Export</h2>

<br />

<table class="table table-striped table-bordered table-hover" id="dataTables-example">
 <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.awbnumber)
        </th>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.hawbnumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.createddate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bls)
        </th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.awbnumber)
        </td>
        <td>
            @Html.ActionLink("Select", "Create", "Manifest", new { id = item.id, tipo_servicio = 22 }, new { @class = "btn btn-primary btn-xs" })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hawbnumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.createddate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bls)
        </td>
    </tr>
}
</tbody>
</table>


<script>
    $(document).ready(function () {
        $('#dataTables-example').dataTable({
            aaSorting: [[0, 'desc']]
        });
    });
</script>

