@model IEnumerable<Manifiesto.Data.Models.listado_bls_VM>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Listado Bls</h2>

<br />

<table class="table table-striped table-bordered table-hover" id="dataTables-example">
 <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.count1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.count2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.no_viaje)
        </th>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.no_bl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_arribo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vapor)
        </th>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {

    <tr>
        <td>       
            @Html.DisplayFor(modelItem => item.count1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.count2)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.tipo)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.no_viaje)
        </td>
        <td>
            @*@Ajax.ActionLink("Select", "Create", new { id = item.id, tipo_servicio = item.tipo }, new AjaxOptions { HttpMethod = "POST", OnSuccess = "success", OnBegin = "AjaxBegin", OnComplete = "AjaxComplete", OnFailure = "AjaxFailure", LoadingElementId = "indicator" }, new { @class = "btn btn-primary btn-xs" })*@
            @Html.ActionLink("Select", "Create", new { id = item.id, tipo_servicio = item.tipo }, new { @class = "btn btn-primary btn-xs" })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.no_bl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_arribo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.vapor)
        </td>
    </tr>
    
}
</tbody>
</table>

<script>
    $(document).ready(function () {
        $('#dataTables-example').dataTable({
            aaSorting: [[0, 'desc']]
        });
    });
</script>
