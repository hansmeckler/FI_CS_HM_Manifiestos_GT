@model IEnumerable<Manifiesto.Web.Areas.Aereo.Models.guiasListViewModel>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Guias Import</h2>

<br />

<table class="table table-striped table-bordered table-hover" id="dataTables-example">
 <thead>
    <tr>
        <th>
            AWB
        </th>
        <th></th>
        <th>
            HAWB
        </th>
        <th>
           Created
        </th>
        <th>
            bls
        </th>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {

    <tr>
        <td>
           @Html.DisplayFor(modelItem => item.awbnumber)
        </td>
        <td>
            @Html.ActionLink("Select", "Create", "Manifest", new { id = item.id, tipo_servicio = 23 }, new { @class = "btn btn-primary btn-xs" })
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.hawbnumber)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.createddate)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.bls)
        </td>
    </tr>
    
}
</tbody>
</table>

<script>
    $(document).ready(function () {
        $('#dataTables-example').dataTable({
            aaSorting: [[0, 'desc']]
        });
    });
</script>
