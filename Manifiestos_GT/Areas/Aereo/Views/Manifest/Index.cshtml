@model IEnumerable<db_aereo.Entities.Entities.cuscar_voyage_info>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<br />

<table class="table table-striped table-bordered table-hover" id="dataTables-example">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cuscar_voyage_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.manifest)
        </th>
        <th>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cuscar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cuscardt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.puerto_origen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.puerto_desembarque)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mtype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mfunction)
        </th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cuscar_voyage_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.manifest)
        </td>
        <td>
            @Html.ActionLink("Select", "Edit", new {  id=item.cuscar_voyage_id  })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cuscar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cuscardt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.puerto_origen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.puerto_desembarque)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mtype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mfunction)
        </td>
    </tr>
}
</tbody>
</table>

<script>
    $(document).ready(function () {
        $('#dataTables-example').dataTable({
            aaSorting: [[0, 'desc']]
        });
    });
</script>

